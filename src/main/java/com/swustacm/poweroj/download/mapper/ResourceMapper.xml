<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.swustacm.poweroj.download.mapper.ResourceMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.swustacm.poweroj.download.entity.Resource">
        <id column="id" property="id" />
        <result column="uid" property="uid" />
        <result column="name" property="name" />
        <result column="description" property="description" />
        <result column="os" property="os" />
        <result column="arch" property="arch" />
        <result column="path" property="path" />
        <result column="ctime" property="ctime" />
        <result column="download" property="download" />
        <result column="access" property="access" />
    </resultMap>
    <select id="getAllResource" resultType="com.swustacm.poweroj.download.entity.Resource">
        select * from resource order by id desc
    </select>

</mapper>
